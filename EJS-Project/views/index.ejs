<%- include("header") -%>
<body>
    <div class="box" id="heading">
        <h1>
            <%= listTitle %>
        </h1>
    </div>
    <div class="box">
        <%for(let item of newListItem){%>
            <div class="item">
                <input type="checkbox">
                <li> <%= item %> </li>
            </div>
        <% } %>
            <form class="item" action="/" method="post">
                <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                <button type="submit" name="button">+</button>
            </form>
    </div>
            
</body>
<%- include("footer") -%>